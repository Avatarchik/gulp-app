    "get-gulp-tasks": "^1.0.1",
    "gulp": "^3.6.2",
    "lodash": "^3.8.0"
  },
  "devDependencies": {
    "electron-packager": "^3.3.0",
    "electron-prebuilt": "^1.2.2",
    "xo": "*"
